{
	"name": "Joona",
	"version": "0.1.0",
	"scripts": {
		"start": "npm run build-production && cd dist && node ./joona.js",
		"build-dev": "webpack --config webpack.client.config.js && webpack --config webpack.server.config.js",
		"build-production": "webpack -p --config webpack.client.config.js && webpack --config webpack.server.config.js",
		"dev": "babel-node ./src/dev.js",
		"test": "npm run lint",
		"lint": "eslint src/",
		"lint-fix": "eslint --fix src/"
	},
	"dependencies": {
		"apollo-client": "^1.9.0-1",
		"read-yaml": "1.1.0",
		"babel-cli": "6.11.4",
		"babel-core": "^6.13.2",
		"babel-loader": "^6.2.5",
		"babel-preset-es2015": "^6.13.2",
		"babel-preset-react": "^6.11.1",
		"babel-preset-stage-0": "^6.5.0",
		"babel-runtime": "^6.11.6",
		"body-parser": "1.17.1",
		"css-loader": "0.28.4",
		"express": "4.15.2",
		"express-session": "1.15.3",
		"graphql-tag": "^2.0.0",
		"react": "^15.4.2",
		"react-apollo": "^1.0.1",
		"react-bootstrap": "^0.30.8",
		"react-dom": "^15.4.2",
		"react-router": "4.1.1",
		"react-router-dom": "^4.1.1",
		"request": "2.81.0",
		"style-loader": "0.18.2",
		"webpack": "^2.6.1",
		"immutability-helper": "2.3.0",
		"draft-js": "0.10.1",
		"query-string": "5.0.0"
	},
	"devDependencies": {
		"babel-watch": "2.0.5",
		"eslint": "4.5.0",
		"eslint-plugin-react": "^7.2.1",
		"grunt-bump": "0.8.0",
		"webpack-dev-middleware": "1.10.2"
	}
}
