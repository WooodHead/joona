{
	"name": "joona",
	"version": "0.2.2",
	"scripts": {
		"start": "npm run build-production && cd dist && node ./joona.js",
		"build-dev": "webpack --config webpack.client.config.js && webpack --config webpack.server.config.js",
		"build-production": "webpack -p --config webpack.client.config.js && webpack --config webpack.server.config.js",
		"analyze": "webpack -p --config webpack.analyze.config.js",
		"dev": "babel-node ./src/dev.js",
		"test": "npm run lint",
		"lint": "eslint src/",
		"lint-fix": "eslint --fix src/",
		"bump-patch": "grunt bump:patch",
		"bump-minor": "grunt bump:minor",
		"bump-major": "grunt bump:major"
	},
	"dependencies": {
		"apollo-client": "^1.9.2",
		"babel-core": "^6.13.2",
		"babel-loader": "^6.2.5",
		"babel-preset-es2015": "^6.13.2",
		"babel-preset-react": "^6.11.1",
		"babel-preset-stage-0": "^6.5.0",
		"babel-runtime": "^6.11.6",
		"body-parser": "1.17.1",
		"css-loader": "0.28.4",
		"draft-js": "0.10.1",
		"express": "4.15.2",
		"express-session": "1.15.5",
		"graphql-tag": "^2.0.0",
		"immutability-helper": "2.3.0",
		"query-string": "5.0.0",
		"react": "^15.4.2",
		"react-apollo": "^1.0.1",
		"react-bootstrap": "^0.30.8",
		"react-dom": "^15.4.2",
		"react-router": "4.1.1",
		"react-router-dom": "^4.1.1",
		"read-yaml": "1.1.0",
		"request": "2.81.0",
		"style-loader": "0.18.2",
		"webpack": "^2.6.1",
		"koodinikkarit-ui-kit": "^0.0.1",
		"react-beautiful-dnd": "^2.3.1"
	},
	"devDependencies": {
		"babel-cli": "6.11.4",
		"babel-eslint": "7.2.3",
		"babel-jest": "^20.0.3",
		"babel-watch": "2.0.5",
		"babel-preset-flow": "^6.23.0",
		"eslint": "4.4.1",
		"eslint-plugin-flowtype": "2.35.0",
		"eslint-plugin-import": "2.7.0",
		"eslint-plugin-jest": "20.0.3",
		"eslint-plugin-react": "7.2.1",
		"jest": "^20.0.4",
		"flow-bin": "^0.51.0",
		"identity-obj-proxy": "3.0.0",
		"webpack-dev-middleware": "1.10.2",
		"webpack-bundle-analyzer": "2.9.0",
		"grunt": "^1.0.1",
		"grunt-bump": "^0.8.0"
	}
}
